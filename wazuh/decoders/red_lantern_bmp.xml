<decoders>
  <decoder name="red_lantern_bmp_route_decoder">
    <prematch>^BMP ROUTE:</prematch>
  </decoder>

  <decoder name="red_lantern_bmp_route_details_decoder">
    <parent>red_lantern_bmp_route_decoder</parent>
    <regex offset="after_parent">
      prefix (\S+) AS_PATH \[([\d,\s]+)\] NEXT_HOP (\S+) ORIGIN_AS (\d+)
    </regex>
    <order>bmp.prefix,bmp.as_path,bmp.next_hop,bmp.origin_as</order>
  </decoder>
</decoders>
